{
    "presets": [
      [
        "@babel/preset-env",
        {
            "useBuiltIns": "usage",
            "targets": {
                "browsers": [
                "> 1%",
                "last 2 versions"
                ]
            },
            "debug":true //调试使用
        }
      ]
    ],
    "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": true
          }
        ]
      ]
  }
  